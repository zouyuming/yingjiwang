.email_box{
    background: url("../../img/bg_keyword_add_input.png") no-repeat scroll 0 0 transparent;
    float: left;
    margin: 10px 12px;
    width: 303px;
}

.window .email_box input{
    display: inline;
    height: 10px;
    width: 20px;
    font-size: 17px;
    text-align:center;
    margin-top: 11px;
}

.window .email_box span{
    display: inline;
    height: 10px;
    width: 20px;
    font-size: 15px;
    text-align:center;
    margin-top: 9px;
}

#email_submit{
    background: url("../../img/bg_keyword_add_out.png") no-repeat scroll 0 0 transparent;
    color: #FFFFFF;
    float: left;
    font-size: 14px;
    height: 29px;
    line-height: 30px;
    margin-right: 10px;
    margin-left: 6px;
    margin-top: 10px;
    overflow: hidden;
    text-align: center;
    text-decoration: none;
    width: 72px;
}

.window .email_span_font span{
    font-size: 17px;
    font-weight: normal;
    margin: 0 7px;
    margin-bottom: 11px;
    
}

.window .email_subject span{
    font-size: 17px;
    font-weight: normal;
    margin: 0 7px;
    margin-bottom: 11px;
}

.email_subject{
    width: 643px;
}

.email_input{
    width: 654px;
}

#email_span_div{
    margin-bottom: 11px;
}

.window .text_area textarea{
    background-color: #E7E7E7;
    margin: 10px 7px;
    width: 100%;
}

.window .email_subject input{
/*    background: url("../img/bg_keyword_add_input.png") no-repeat scroll 0 0 transparent;*/
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #CCCCCC;
    clear: both;
    display: inline;
    font-size: 16px;
    height: 28px;
    line-height: 28px;
    margin-bottom: 20px;
    padding: 5px;
    width: 531px;
}

#email_div{
    margin-top: 3px;
    margin-left: 7px;
    width: 600px;
}


.email_span_font {
    margin-left: 30px;
    margin-top: 9px;
}